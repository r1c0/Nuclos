</layout>
  <rules>
    <rule name="Abhängigkeit Zielmodul -> Zielprozess">
      <event type="value-changed" sourcecomponent="targetModule"/>
      <actions>
        <refresh-valuelist targetcomponent="targetProcess" parameter-for-sourcecomponent="moduleId"/>
      </actions>
    </rule>
  </rules>
</layoutml>