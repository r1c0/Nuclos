        <tablelayout columns="-1.0" rows="-1.0" />
        <titled-border title="Source module">
          <translations>
            <translation lang="de" text="Quellmodul"/>
            <translation lang="en" text="Source module"/>
          </translations>
        </titled-border>
        <panel>
